/* Exemplo de Programa na linguagem L */

int n;
string nome;
boolean naoTerminou;
final MAXITER<-10;

/* Programa Principal */

write("Digite seu nome: ");
readln(nome);
naoTerminou<-TRUE &&& FALSE;
n<-0;
while (naoTerminou) begin
  writeln("Ola ",nome);
  n<-n+1;
  naoTerminou<-n<MAXITER;
endwhile